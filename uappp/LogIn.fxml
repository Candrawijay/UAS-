<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uappp.LogInController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="360.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/Android%20Large%20-%204.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfUserid" layoutX="30.0" layoutY="422.0" prefHeight="40.0" prefWidth="302.0" promptText="User ID" />
      <PasswordField fx:id="tfPass" layoutX="30.0" layoutY="504.0" prefHeight="40.0" prefWidth="302.0" promptText="Password" />
      <Button fx:id="btnLogIn" layoutX="138.0" layoutY="588.0" mnemonicParsing="false" onAction="#btnLogIn" opacity="0.0" prefHeight="40.0" prefWidth="83.0" text="Button" />
      <Button fx:id="btnBack" layoutX="24.0" layoutY="47.0" mnemonicParsing="false" onAction="#btnBack" opacity="0.0" prefHeight="26.0" prefWidth="37.0" text="Button" />
   </children>
</AnchorPane>
