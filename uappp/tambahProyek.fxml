      <TextField fx:id="tfNama" layoutX="30.0" layoutY="315.0" prefHeight="40.0" prefWidth="300.0" promptText="Nama Proyek" />
      <TextField fx:id="tfTenggat" layoutX="30.0" layoutY="405.0" prefHeight="40.0" prefWidth="300.0" promptText="Tenggat" />
      <TextField fx:id="tfAnggaran" layoutX="30.0" layoutY="495.0" prefHeight="40.0" prefWidth="300.0" promptText="Biaya" />
      <Button fx:id="btnTambah" layoutX="30.0" layoutY="741.0" mnemonicParsing="false" onAction="#btnTambah" opacity="0.0" prefHeight="40.0" prefWidth="300.0" text="Button" />
